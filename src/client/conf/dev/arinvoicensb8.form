	AND VAS.TAX_TYPE_ID = ARI.TAX_TYPE_ID \
	AND ARI.TAX_TYPE_ID IN (1, 4, 5, 6, 8, 9) \
	AND AR.COMPANY_ID = IN_COMPANY_ID
