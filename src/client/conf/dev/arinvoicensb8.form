ArInvoice7.sql.1.je=SELECT 'AR INVOICE - NSB 8' as SOURCE, AR.AR_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, A.ACCOUNT_ID, \
	D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AR.DATE_RECEIVED as GL_DATE, \
	CONCAT('AR', AR.SEQUENCE_NO) as REFERENCE_NUMBER, AR.REMARKS AS DESCRIPTION, \
	0 AS DEBIT, ARI.AMOUNT + COALESCE(ARI.DISCOUNT, 0) as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, \
	COALESCE((SELECT SO.PO_NUMBER FROM SALES_ORDER SO INNER JOIN DELIVERY_RECEIPT DR ON DR.SALES_ORDER_ID = SO.SALES_ORDER_ID \
	WHERE DR.DELIVERY_RECEIPT_ID = AR.DELIVERY_RECEIPT_ID), '') AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM AR_INVOICE_ITEM ARI \
	INNER JOIN AR_INVOICE AR ON AR.AR_INVOICE_ID = ARI.AR_INVOICE_ID \
	INNER JOIN ITEM I ON I.ITEM_ID = ARI.ITEM_ID \
	INNER JOIN ITEM_CATEGORY_ACCOUNT_SETUP ICAS ON I.ITEM_CATEGORY_ID = ICAS.ITEM_CATEGORY_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON (AC.ACCOUNT_COMBINATION_ID = ICAS.SALES_ACCOUNT AND AC.COMPANY_ID = AR.COMPANY_ID) \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = AR.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
	AND ARI.AMOUNT != 0 \
	AND AR.AR_INVOICE_TYPE_ID = 7 \
	AND ICAS.COMPANY_ID = AC.COMPANY_ID \
	AND AC.DIVISION_ID = AR.DIVISION_ID \
	AND AC.COMPANY_ID = IN_COMPANY_ID
ArInvoice7.sql.2.je=SELECT 'AR INVOICE - NSB 8' as SOURCE, AR.AR_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, A.ACCOUNT_ID, \
	D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AR.DATE_RECEIVED as GL_DATE, \
