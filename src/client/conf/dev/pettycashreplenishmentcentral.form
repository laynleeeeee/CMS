# VAT
APInvoice55.sql.6.je=SELECT 'PETTY CASH REPLENISHMENT - CENTRAL' as SOURCE, AI.AP_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, \
	AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AI.GL_DATE, \
	CONCAT('PCR ', AI.SEQUENCE_NO) as REFERENCE_NUMBER, COALESCE(PCVL.DESCRIPTION, '') AS DESCRIPTION, IF(PCVL.AMOUNT > 0, COALESCE(PCVL.VAT_AMOUNT, 0), 0) AS DEBIT, \
	IF(PCVL.AMOUNT < 0, ABS(COALESCE(PCVL.VAT_AMOUNT, 0)), 0) as CREDIT, FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, \
	FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM PETTY_CASH_VOUCHER_LIQUIDATION_LINE PCVL \
	INNER JOIN OBJECT_TO_OBJECT OTO ON OTO.FROM_OBJECT_ID = PCVL.EB_OBJECT_ID \
	INNER JOIN PETTY_CASH_REPLENISHMENT_LINE PCRL ON PCRL.EB_OBJECT_ID = OTO.TO_OBJECT_ID \
	INNER JOIN AP_INVOICE AI ON AI.AP_INVOICE_ID = PCRL.AP_INVOICE_ID \
	INNER JOIN SUPPLIER_ACCOUNT SA ON SA.SUPPLIER_ACCOUNT_ID = AI.SUPPLIER_ACCOUNT_ID \
	INNER JOIN VAT_ACCOUNT_SETUP VAS ON IF(AI.DIVISION_ID IS NOT NULL, \
	(VAS.COMPANY_ID = AI.COMPANY_ID AND VAS.DIVISION_ID = AI.DIVISION_ID), VAS.COMPANY_ID = AI.COMPANY_ID) \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = VAS.INPUT_VAT_AC_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON AI.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	WHERE AI.INVOICE_TYPE_ID = 55 \
	AND AI.DIVISION_ID = 1 \
	AND OTO.OR_TYPE_ID = 24008 \
	AND VAS.TAX_TYPE_ID = PCVL.TAX_TYPE_ID \
	AND PCVL.TAX_TYPE_ID IN (1, 4, 5, 6) \
	AND FW.CURRENT_STATUS_ID != 4 \
	AND AC.COMPANY_ID = IN_COMPANY_ID
APInvoice55.sql.je.jerr=AND AI.GL_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND (CASE WHEN IN_REFERENCE_NUMBER NOT LIKE '-1' THEN CONCAT('PCR ', AI.SEQUENCE_NO) LIKE IN_REFERENCE_NUMBER \
		ELSE IN_REFERENCE_NUMBER LIKE IN_REFERENCE_NUMBER END) \
	AND (CASE IN_SOURCE WHEN 'PETTY CASH REPLENISHMENT - CENTRAL' THEN AI.AP_INVOICE_ID != 0 \
		WHEN 'ALL' THEN AI.AP_INVOICE_ID != 0 ELSE AI.AP_INVOICE_ID = 0 END) \
	AND (CASE WHEN IN_IS_POSTED != -1 THEN FW.IS_COMPLETE = IN_IS_POSTED ELSE FW.IS_COMPLETE != IN_IS_POSTED END)
APInvoice55.sql.je.ab=AND IF(DATE_FROM IS NOT NULL, AI.GL_DATE BETWEEN DATE_FROM AND DATE_TO, AI.GL_DATE <= DATE_TO) \
	AND IF(IN_DIVISION_ID != -1, AI.DIVISION_ID = IN_DIVISION_ID, AI.DIVISION_ID != IN_DIVISION_ID) \
	AND FW.IS_COMPLETE = 1
APInvoice55.sql.je.aa=AND D.NUMBER BETWEEN IN_FROM_DIVISION AND IN_TO_DIVISION \
	AND AC.ACCOUNT_ID = IN_ACCOUNT_ID \
	AND AI.GL_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND FW.IS_COMPLETE = 1
APInvoice55.sql.je.gll=AND AC.ACCOUNT_ID = IN_ACCOUNT_ID \
	AND AI.GL_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND FW.IS_COMPLETE = 1
