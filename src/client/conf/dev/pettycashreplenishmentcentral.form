# AP Invoice - Debit
APInvoice55.sql.1.je=SELECT 'PETTY CASH REPLENISHMENT - CENTRAL' as SOURCE, AI.AP_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, \
	AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AI.GL_DATE, \
	CONCAT('PCR ', AI.SEQUENCE_NO) as REFERENCE_NUMBER, COALESCE(PCVL.DESCRIPTION, '') AS DESCRIPTION, PCVL.AMOUNT AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM PETTY_CASH_VOUCHER_LIQUIDATION_LINE PCVL \
	INNER JOIN OBJECT_TO_OBJECT OTO ON OTO.FROM_OBJECT_ID = PCVL.EB_OBJECT_ID \
	INNER JOIN PETTY_CASH_REPLENISHMENT_LINE PCRL ON PCRL.EB_OBJECT_ID = OTO.TO_OBJECT_ID \
	INNER JOIN AP_INVOICE AI ON AI.AP_INVOICE_ID = PCRL.AP_INVOICE_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = PCVL.ACCOUNT_COMBINATION_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON AI.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	WHERE PCVL.AMOUNT >= 0 \
	AND AI.INVOICE_TYPE_ID = 55 \
	AND AI.DIVISION_ID = 1 \
	AND OTO.OR_TYPE_ID = 24008 \
	AND FW.CURRENT_STATUS_ID != 4 \
	AND AC.COMPANY_ID = IN_COMPANY_ID
# AP Invoice - Debit
APInvoice55.sql.2.je=SELECT 'PETTY CASH REPLENISHMENT - CENTRAL' as SOURCE, AI.AP_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, \
	AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AI.GL_DATE, \
	CONCAT('PCR ', AI.SEQUENCE_NO) as REFERENCE_NUMBER, AI.DESCRIPTION, -AI.AMOUNT as DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM AP_INVOICE AI \
	INNER JOIN SUPPLIER_ACCOUNT SA ON SA.SUPPLIER_ACCOUNT_ID = AI.SUPPLIER_ACCOUNT_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = SA.DEFAULT_CREDIT_AC_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON AI.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	WHERE AI.AMOUNT < 0 \
	AND AI.INVOICE_TYPE_ID = 55 \
	AND AI.DIVISION_ID = 1 \
	AND AC.COMPANY_ID = SA.COMPANY_ID \
	AND FW.CURRENT_STATUS_ID != 4 \
	AND AC.COMPANY_ID = IN_COMPANY_ID
# Withholding Tax
APInvoice55.sql.3.je=SELECT 'PETTY CASH REPLENISHMENT - CENTRAL' as SOURCE, AI.AP_INVOICE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, \
	AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AI.GL_DATE, \
	CONCAT('PCR ', AI.SEQUENCE_NO) as REFERENCE_NUMBER, AI.DESCRIPTION, IF(AI.AMOUNT < 0, ABS(AI.WT_AMOUNT), 0) AS DEBIT, \
	IF(AI.AMOUNT > 0, AI.WT_AMOUNT, 0) as CREDIT, FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, \
	FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM AP_INVOICE AI \
	INNER JOIN WT_ACCOUNT_SETTING WTAS ON WTAS.WT_ACCOUNT_SETTING_ID = AI.WT_ACCOUNT_SETTING_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = WTAS.ACCOUNT_COMBINATION_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON AI.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	WHERE AI.INVOICE_TYPE_ID = 55 \
	AND AI.DIVISION_ID = 1 \
	AND AI.WT_ACCOUNT_SETTING_ID IS NOT NULL \
	AND FW.CURRENT_STATUS_ID != 4 \
	AND AC.COMPANY_ID = IN_COMPANY_ID
