DeliveryReceipt1.sql.4.je=SELECT 'DELIVERY RECEIPT - GOODS' as SOURCE, DR.DELIVERY_RECEIPT_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, AC.ACCOUNT_ID, \
	D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, DR.DATE as GL_DATE, \
