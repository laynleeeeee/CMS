	IF(AL.AMOUNT >= 0, COALESCE(AL.VAT_AMOUNT, 0), 0) AS DEBIT, IF(AL.AMOUNT < 0, ABS(COALESCE(AL.VAT_AMOUNT, 0)), 0) as CREDIT, \
