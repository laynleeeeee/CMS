#Cash Sales - Receipt Method - Default Debit - Sale Items
CashSale1.sql.5.je=SELECT 'CASH SALES' as SOURCE, CS.CASH_SALE_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, AC.ACCOUNT_ID, \
	D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, CS.RECEIPT_DATE as GL_DATE, \
	CONCAT('CS', CS_NUMBER) as REFERENCE_NUMBER, CS.SALE_INVOICE_NO AS DESCRIPTION, \
	CSI.AMOUNT + COALESCE(CSI.VAT_AMOUNT, 0) AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
