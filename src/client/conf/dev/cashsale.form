	INNER JOIN VAT_ACCOUNT_SETUP IA ON IA.OUTPUT_VAT_AC_ID = AC.ACCOUNT_COMBINATION_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
