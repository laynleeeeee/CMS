	ASRI.UNIT_COST AS IN_UNIT_COST, 0 AS OUT_UNIT_COST, ABS(ASRI.UNIT_COST * ASRI.QUANTITY) AS IN_TOTAL_COST, 0 AS OUT_TOTAL_COST, \
	ART.TRANSACTION_DATE AS DATE, ART.CREATED_DATE, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, I.ACTIVE, \
