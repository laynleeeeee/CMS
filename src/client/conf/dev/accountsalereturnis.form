ArTransaction11.sql.1.inv=SELECT 'ACCOUNT SALE RETURN - IS' AS SOURCE, ASRI.WAREHOUSE_ID AS WAREHOUSE_ID, ASRI.ITEM_ID AS ITEM_ID, I.STOCK_CODE AS STOCK_CODE, \
