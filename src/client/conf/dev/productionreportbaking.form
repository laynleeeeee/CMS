ProcessingReport5.sql.1.inv=SELECT 'PROCESSING REPORT' AS SOURCE, PRI.WAREHOUSE_ID, PRI.ITEM_ID, \
	I.STOCK_CODE, I.DESCRIPTION, I.ITEM_CATEGORY_ID, I.UNIT_MEASUREMENT_ID, \
	0 AS IN_QUANTITY, PRI.QUANTITY AS OUT_QUANTITY, 0 AS IN_UNIT_COST, PRI.UNIT_COST AS OUT_UNIT_COST, \
	0 AS IN_TOTAL_COST, (PRI.QUANTITY * COALESCE(PRI.UNIT_COST, 0)) AS OUT_TOTAL_COST, \
	PR.DATE AS DATE, PR.CREATED_DATE, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, I.ACTIVE, \
	CONCAT('PR-RM ', PR.SEQUENCE_NO) AS REFERENCE_NUMBER, '' AS INVOICE_NUMBER, 'RAW MATERIAL' AS TRANS_DESCRIPTION, \
	0 AS SRP, PR.EB_OBJECT_ID AS PARENT_EB_OBJECT_ID, PRI.EB_OBJECT_ID AS EB_OBJECT_ID \
	FROM PR_RAW_MATERIALS_ITEM PRI \
	INNER JOIN PROCESSING_REPORT PR ON PR.PROCESSING_REPORT_ID = PRI.PROCESSING_REPORT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = PR.FORM_WORKFLOW_ID \
	INNER JOIN ITEM I ON I.ITEM_ID = PRI.ITEM_ID \
	WHERE PR.PROCESSING_REPORT_TYPE_ID = 5 \
	AND PR.COMPANY_ID = IN_COMPANY_ID
ProcessingReport5.sql.2.inv=SELECT 'PROCESSING REPORT' AS SOURCE, PRI.WAREHOUSE_ID, PRI.ITEM_ID, \
	I.STOCK_CODE, I.DESCRIPTION, I.ITEM_CATEGORY_ID, I.UNIT_MEASUREMENT_ID, \
	PRI.QUANTITY AS IN_QUANTITY, 0 AS OUT_QUANTITY, PRI.UNIT_COST AS IN_UNIT_COST, 0 AS OUT_UNIT_COST, \
	(PRI.QUANTITY * COALESCE(PRI.UNIT_COST, 0)) AS IN_TOTAL_COST, 0 AS OUT_TOTAL_COST, \
	PR.DATE AS DATE, PR.CREATED_DATE, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, I.ACTIVE, \
	CONCAT('PR-MP ', PR.SEQUENCE_NO) AS REFERENCE_NUMBER, '' AS INVOICE_NUMBER, 'MAIN PRODUCT' AS TRANS_DESCRIPTION, \
	0 AS SRP, PR.EB_OBJECT_ID AS PARENT_EB_OBJECT_ID, PRI.EB_OBJECT_ID AS EB_OBJECT_ID \
	FROM PR_MAIN_PRODUCT PRI \
	INNER JOIN PROCESSING_REPORT PR ON PR.PROCESSING_REPORT_ID = PRI.PROCESSING_REPORT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = PR.FORM_WORKFLOW_ID \
	INNER JOIN ITEM I ON I.ITEM_ID = PRI.ITEM_ID \
	WHERE PR.PROCESSING_REPORT_TYPE_ID = 5 \
	AND PR.COMPANY_ID = IN_COMPANY_ID
ProcessingReport5.sql.inv.as=AND FW.IS_COMPLETE = 1 \
	AND PRI.ITEM_ID = IN_ITEM_ID \
	AND PRI.WAREHOUSE_ID = IN_WAREHOUSE_ID \
	AND PR.DATE <= IN_DATE
ProcessingReport5.sql.inv.rsf=AND FW.IS_COMPLETE = 1 \
	AND PRI.ITEM_ID = IN_ITEM_ID \
	AND PRI.WAREHOUSE_ID = IN_WAREHOUSE_ID \
	AND PR.DATE > IN_DATE
ProcessingReport5.sql.inv.il=AND PR.DATE <= IN_DATE \
	AND IF(IN_ITEM_CATEGORY_ID != -1, I.ITEM_CATEGORY_ID = IN_ITEM_CATEGORY_ID, I.ITEM_CATEGORY_ID != 0) \
	AND IF(IN_WAREHOUSE_ID != -1, PRI.WAREHOUSE_ID = IN_WAREHOUSE_ID, PRI.WAREHOUSE_ID != IN_WAREHOUSE_ID) \
	AND IF(IN_IS_ACTIVE != -1, I.ACTIVE = IN_IS_ACTIVE, I.ACTIVE != IN_IS_ACTIVE ) \
	AND CASE WHEN IN_STATUS = -1 THEN FW.CURRENT_STATUS_ID != 4 WHEN IN_STATUS = 1 THEN FW.IS_COMPLETE = 1 END
ProcessingReport5.sql.inv.sc=AND FW.IS_COMPLETE = 1 \
	AND PRI.ITEM_ID = IN_ITEM_ID \
	AND PRI.WAREHOUSE_ID = IN_WAREHOUSE_ID \
	AND PR.DATE BETWEEN IN_START_DATE AND IN_END_DATE
ProcessingReport5.sql.inv.ft=AND FW.IS_COMPLETE = 1 \
	AND PRI.ITEM_ID = IN_ITEM_ID \
