# Withholding VAT
ArMiscellaneous3.sql.7.je=SELECT 'OTHER RECEIPT - NSB 4' as SOURCE, AM.AR_MISCELLANEOUS_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, AM.MATURITY_DATE as GL_DATE, AM.RECEIPT_NUMBER AS REFERENCE_NUMBER, AM.DESCRIPTION, COALESCE(AM.WT_VAT_AMOUNT, 0) AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM AR_MISCELLANEOUS AM \
	INNER JOIN AR_MISCELLANEOUS_ACCOUNT ARMA ON (ARMA.COMPANY_ID = AM.COMPANY_ID AND ARMA.DIVISION_ID = AM.DIVISION_ID) \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = ARMA.WT_VAT_AC_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON AM.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
	AND AM.DIVISION_ID = 3 \
	AND AM.WT_VAT_AMOUNT > 0 \
	AND AC.COMPANY_ID = IN_COMPANY_ID
ArMiscellaneous3.sql.je.jerr=AND AM.MATURITY_DATE BETWEEN DATE_FROM and DATE_TO \
	AND (CASE WHEN IN_REFERENCE_NUMBER NOT LIKE '-1' THEN AM.RECEIPT_NUMBER like IN_REFERENCE_NUMBER \
		ELSE IN_REFERENCE_NUMBER like IN_REFERENCE_NUMBER END) \
	AND (CASE IN_SOURCE WHEN 'OTHER RECEIPT - NSB 4' THEN AM.AR_MISCELLANEOUS_ID != 0 \
		WHEN 'ALL' THEN AM.AR_MISCELLANEOUS_ID != 0 \
		ELSE AM.AR_MISCELLANEOUS_ID = 0 END) \
	AND (CASE WHEN IN_IS_POSTED != -1 THEN FW.IS_COMPLETE = IN_IS_POSTED ELSE FW.IS_COMPLETE != IN_IS_POSTED END)
ArMiscellaneous3.sql.je.ab=AND IF(DATE_FROM IS NOT NULL, AM.MATURITY_DATE BETWEEN DATE_FROM AND DATE_TO, AM.MATURITY_DATE <= DATE_TO) \
	AND IF(IN_DIVISION_ID != -1, AM.DIVISION_ID = IN_DIVISION_ID, AM.DIVISION_ID != IN_DIVISION_ID) \
	AND FW.IS_COMPLETE = 1
ArMiscellaneous3.sql.je.aa=AND D.NUMBER BETWEEN IN_FROM_DIVISION AND IN_TO_DIVISION \
	AND AC.ACCOUNT_ID = IN_ACCOUNT_ID \
	AND AM.MATURITY_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND AM.DESCRIPTION LIKE IN_DESCRIPTION \
	AND FW.IS_COMPLETE = 1
ArMiscellaneous3.sql.je.gll=AND AC.ACCOUNT_ID = IN_ACCOUNT_ID \
	AND AM.MATURITY_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND FW.IS_COMPLETE = 1
