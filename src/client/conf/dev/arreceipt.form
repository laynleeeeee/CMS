ArReceipt.sql.12.je=SELECT 'ACCOUNT COLLECTION' AS SOURCE, AR.AR_RECEIPT_ID AS ID, \
	AC.COMPANY_ID, AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
