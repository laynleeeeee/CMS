# Cash - Debit
LoanProceeds3.sql.1.je=SELECT 'LOAN PROCEEDS - NSB 4' as SOURCE, LP.LOAN_PROCEEDS_ID as ID, LP.COMPANY_ID, \
	AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, LP.GL_DATE, CONCAT('LP ', LP.SEQUENCE_NO), \
	LP.DESCRIPTION as DESCRIPTION, LP.AMOUNT - COALESCE(LP.WT_AMOUNT) - (SELECT SUM(LPL.GROSS_AMOUNT) FROM LP_LINE LPL \
	WHERE LPL.LOAN_PROCEEDS_ID = LP.LOAN_PROCEEDS_ID) AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM LOAN_PROCEEDS LP \
	INNER JOIN RECEIPT_METHOD RM ON RM.RECEIPT_METHOD_ID = LP.RECEIPT_METHOD_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = RM.DEBIT_ACCOUNT_COMBINATION_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = LP.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
	AND LP.LOAN_PROCEEDS_TYPE_ID = 3 \
	AND LP.COMPANY_ID = AC.COMPANY_ID \
	AND AC.COMPANY_ID = IN_COMPANY_ID
# Bank Charges - Debit
LoanProceeds3.sql.2.je=SELECT 'LOAN PROCEEDS - NSB 4' as SOURCE, LP.LOAN_PROCEEDS_ID as ID, LP.COMPANY_ID, \
	AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, LP.GL_DATE, CONCAT('LP ', LP.SEQUENCE_NO), \
	LP.DESCRIPTION as DESCRIPTION, LPL.AMOUNT AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM LOAN_PROCEEDS LP \
	INNER JOIN LP_LINE LPL ON LPL.LOAN_PROCEEDS_ID = LP.LOAN_PROCEEDS_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = LPL.ACCOUNT_COMBINATION_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = LP.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
	AND LP.LOAN_PROCEEDS_TYPE_ID = 3 \
	AND LP.COMPANY_ID = AC.COMPANY_ID \
	AND AC.COMPANY_ID = IN_COMPANY_ID
# VAT - Debit
LoanProceeds3.sql.3.je=SELECT 'LOAN PROCEEDS - NSB 4' as SOURCE, LP.LOAN_PROCEEDS_ID as ID, LP.COMPANY_ID, \
	AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, LP.GL_DATE, CONCAT('LP ', LP.SEQUENCE_NO), \
	LP.DESCRIPTION as DESCRIPTION, LPL.VAT_AMOUNT AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM LOAN_PROCEEDS LP \
    INNER JOIN LP_LINE LPL ON LPL.LOAN_PROCEEDS_ID = LP.LOAN_PROCEEDS_ID \
    INNER JOIN VAT_ACCOUNT_SETUP VAS ON (VAS.COMPANY_ID = LP.COMPANY_ID AND VAS.DIVISION_ID = LP.DIVISION_ID) \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = VAS.INPUT_VAT_AC_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = LP.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
    AND VAS.TAX_TYPE_ID = LPL.TAX_TYPE_ID \
	AND LP.LOAN_PROCEEDS_TYPE_ID = 3 \
	AND LP.COMPANY_ID = AC.COMPANY_ID \
	AND AC.COMPANY_ID = IN_COMPANY_ID
