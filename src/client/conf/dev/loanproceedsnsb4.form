# Bank account - Debit
LoanProceeds3.sql.6.je=SELECT 'LOAN PROCEEDS - NSB 4' as SOURCE, LP.LOAN_PROCEEDS_ID as ID, LP.COMPANY_ID, \
	AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
	A.NUMBER as ACCT_NO, A.ACCOUNT_NAME, LP.GL_DATE, CONCAT('LP ', LP.SEQUENCE_NO) as REFERENCE_NUMBER , \
	LP.DESCRIPTION as DESCRIPTION, COALESCE(LP.LOAN_PROCEED_AMOUNT, 0) AS DEBIT, 0 as CREDIT, \
	FW.FORM_WORKFLOW_ID, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, FW.IS_COMPLETE as IS_POSTED, '' AS CUSTOMER_PO_NO, A.ACCOUNT_TYPE_ID \
	FROM LOAN_PROCEEDS LP \
	INNER JOIN RECEIPT_METHOD RM ON RM.RECEIPT_METHOD_ID = LP.RECEIPT_METHOD_ID \
	INNER JOIN BANK_ACCOUNT BA ON BA.BANK_ACCOUNT_ID = RM.BANK_ACCOUNT_ID \
	INNER JOIN ACCOUNT_COMBINATION AC ON AC.ACCOUNT_COMBINATION_ID = BA.CASH_IN_BANK_ACCT_ID \
	INNER JOIN DIVISION D ON D.DIVISION_ID = AC.DIVISION_ID \
	INNER JOIN ACCOUNT A ON A.ACCOUNT_ID = AC.ACCOUNT_ID \
	INNER JOIN FORM_WORKFLOW FW ON FW.FORM_WORKFLOW_ID = LP.FORM_WORKFLOW_ID \
	WHERE FW.CURRENT_STATUS_ID != 4 \
	AND LP.LOAN_PROCEEDS_TYPE_ID = 3 \
	AND LP.COMPANY_ID = AC.COMPANY_ID \
	AND AC.COMPANY_ID = IN_COMPANY_ID
LoanProceeds3.sql.je.jerr=AND LP.GL_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND (CASE WHEN IN_REFERENCE_NUMBER NOT LIKE '-1' THEN CONCAT('LP ', LP.SEQUENCE_NO) LIKE IN_REFERENCE_NUMBER \
		ELSE IN_REFERENCE_NUMBER LIKE IN_REFERENCE_NUMBER END) \
	AND (CASE IN_SOURCE WHEN 'LOAN PROCEEDS - NSB 4' THEN LP.LOAN_PROCEEDS_ID != 0 \
		WHEN 'ALL' THEN LP.LOAN_PROCEEDS_ID != 0 ELSE LP.LOAN_PROCEEDS_ID = 0 END) \
	AND (CASE WHEN IN_IS_POSTED != -1 THEN FW.IS_COMPLETE = IN_IS_POSTED ELSE FW.IS_COMPLETE != IN_IS_POSTED END)
LoanProceeds3.sql.je.ab=AND IF(DATE_FROM IS NOT NULL, LP.GL_DATE BETWEEN DATE_FROM AND DATE_TO, LP.GL_DATE <= DATE_TO) \
	AND IF(IN_DIVISION_ID != -1, LP.DIVISION_ID = IN_DIVISION_ID, LP.DIVISION_ID != IN_DIVISION_ID) \
	AND FW.IS_COMPLETE = 1
LoanProceeds3.sql.je.aa=AND D.NUMBER BETWEEN IN_FROM_DIVISION AND IN_TO_DIVISION \
	AND AC.ACCOUNT_ID = IN_ACCOUNT_ID \
	AND LP.GL_DATE BETWEEN DATE_FROM AND DATE_TO \
	AND LP.DESCRIPTION LIKE IN_DESCRIPTION \
	AND FW.IS_COMPLETE = 1
