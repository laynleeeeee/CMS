ArTransaction5.sql.2.inv=SELECT 'ACCOUNT SALE RETURN' AS SOURCE, ASRI.WAREHOUSE_ID AS WAREHOUSE_ID, ASRI.ITEM_ID AS ITEM_ID, I.STOCK_CODE AS STOCK_CODE, \
	I.DESCRIPTION AS DESCRIPTION, I.ITEM_CATEGORY_ID AS ITEM_CATEGORY_ID, I.UNIT_MEASUREMENT_ID as UNIT_MEASUREMENT_ID, 0 AS IN_QUANTITY, ASRI.QUANTITY AS OUT_QUANTITY, \
	0 AS IN_UNIT_COST, ASRI.UNIT_COST AS OUT_UNIT_COST, 0 AS IN_TOTAL_COST, ASRI.UNIT_COST * ASRI.QUANTITY AS OUT_TOTAL_COST, \
	ART.TRANSACTION_DATE AS DATE, ART.CREATED_DATE, FW.CURRENT_STATUS_ID, FW.IS_COMPLETE, I.ACTIVE, \
	CONCAT('ASR', ART.SEQUENCE_NO) as REFERENCE_NUMBER, \
	'' AS INVOICE_NUMBER, C.NAME AS TRANSACTION_DESCRIPTION, ASRI.SRP AS SRP, \
	ART.EB_OBJECT_ID as PARENT_EB_OBJECT_ID, ASRI.EB_OBJECT_ID as EB_OBJECT_ID  \
	FROM ACCOUNT_SALE_ITEM ASRI \
	INNER JOIN AR_TRANSACTION ART ON ART.AR_TRANSACTION_ID = ASRI.AR_TRANSACTION_ID \
	INNER JOIN AR_CUSTOMER C ON C.AR_CUSTOMER_ID = ART.CUSTOMER_ID \
	INNER JOIN FORM_WORKFLOW FW ON ART.FORM_WORKFLOW_ID = FW.FORM_WORKFLOW_ID \
	INNER JOIN ITEM I ON ASRI.ITEM_ID = I.ITEM_ID \
	WHERE ASRI.QUANTITY >= 0 \
	AND ART.AR_TRANSACTION_TYPE_ID = 5 \
	AND COMPANY_ID = IN_COMPANY_ID
