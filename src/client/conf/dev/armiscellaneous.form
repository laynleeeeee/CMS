# VAT
ArMiscellaneous.sql.5.je=SELECT 'OTHER RECEIPT' as SOURCE, AM.AR_MISCELLANEOUS_ID as ID, AC.COMPANY_ID, AC.DIVISION_ID, AC.ACCOUNT_ID, D.NUMBER AS DIVISION_NO, D.NAME AS DIVISION_NAME, \
